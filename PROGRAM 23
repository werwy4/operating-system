#include <stdio.h>

int main() {
    int blocks[] = {100, 500, 200, 300, 600}, processes[] = {212, 417, 112, 426};
    int n = 5, m = 4;
    
    for (int i = 0; i < m; i++) {
        int allocated = 0;
        for (int j = 0; j < n; j++) {
            if (blocks[j] >= processes[i]) {
                printf("P%d -> B%d\n", i + 1, j + 1);
                blocks[j] -= processes[i];
                allocated = 1;
                break;
            }
        }
        if (!allocated) printf("P%d -> Not Allocated\n", i + 1);
    }
    return 0;
}
OUTPUT:
P1 -> B2
P2 -> B5
P3 -> B2
P4 -> Not Allocated
